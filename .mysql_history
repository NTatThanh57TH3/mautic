CREATE DATABASE mautic;
CREATE USER 'mauticuser'@'localhost' IDENTIFIED BY 'new_password_here';
GRANT ALL ON mautic.* TO 'mauticuser'@'localhost' IDENTIFIED BY 'user_password_here' WITH GRANT OPTION;
FLUSH PRIVILEGES;
EXIT;
CREATE USER 'mautic'@'localhost' IDENTIFIED BY 'thanhngjn123';
GRANT ALL ON mautic.* TO 'mautic'@'localhost' IDENTIFIED BY 'thanhngjn123' WITH GRANT OPTION;
FLUSH PRIVILEGES;
EXIT;
